{
  "id": "openclaw-todo",
  "name": "openclaw-todo",
  "version": "0.1.0",
  "description": "Bridge plugin that forwards /todo commands to the Python HTTP server",
  "main": "index.ts",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "serverUrl": {
        "type": "string",
        "minLength": 1,
        "default": "http://127.0.0.1:8200",
        "description": "Python TODO server URL"
      }
    }
  }
}
